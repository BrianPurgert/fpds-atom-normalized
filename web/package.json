{
  "name": "fpds-search",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bunx vite dev",
    "build": "bunx vite build",
    "build:cf": "bunx vite build && node_modules/.bin/esbuild cf-entry.js --bundle --format=esm --platform=browser --main-fields=module,main --outfile=dist/client/_worker.js --conditions=workerd,worker,import,module \"--external:node:*\"",
    "deploy": "npm run build:cf && node_modules/.bin/wrangler pages deploy dist/client",
    "preview:cf": "npm run build:cf && node_modules/.bin/wrangler pages dev dist/client",
    "start": "node dist/server/server.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.95.3",
    "@tanstack/react-router": "^1.160.0",
    "@tanstack/react-start": "^1.160.0",
    "pg": "^8.18.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^6.1.1",
    "wrangler": "^4.66.0"
  }
}
